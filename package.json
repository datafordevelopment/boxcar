{
  "name": "boxcar",
  "version": "0.0.1",
  "description": "A choo view for handling lists of data",
  "main": "boxcar.js",
  "scripts": {
    "dep": "dependency-check . && dependency-check . --extra --no-dev -i es2020",
    "start": "budo demo.js -o --title \"Boxcar Demo\" -P -l -- -t -g=es2020 -t sheetify/transform",
    "test": "npm run dep && browserify -t es2020 -t sheetify/transform test/*.spec.js | tape-run",
    "test:cov": "browserify -t es2020 -t sheetify/transform -p tape-istanbul/plugin test/*.spec.js | tape-run | tape-istanbul && istanbul report && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:sauce": "zuul -- test/*.spec.js",
    "test:local": "zuul --local 8080 -- test/*.spec.js",
    "build": "browserify . > bundle.js"
  },
  "browserify": {
    "transforms": [
      "sheetify/transform",
      [
        "es2020",
        {
          "global": true
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com:toddself/boxcar.git"
  },
  "keywords": [
    "slickgrid",
    "table",
    "sortable",
    "choo",
    "grid"
  ],
  "author": "Todd Kennedy <todd@selfassembled.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/toddself/boxcar/issues"
  },
  "homepage": "https://github.com/toddself/boxcar#readme",
  "dependencies": {
    "choo": "^2.3.1",
    "es2020": "^1.1.6",
    "pull-notify": "^0.1.1",
    "pull-stream": "^3.4.3",
    "sheetify": "^5.0.3",
    "shortid": "^2.2.6",
    "string.fromcodepoint": "^0.2.1"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "budo": "^8.3.0",
    "coveralls": "^2.11.9",
    "dependency-check": "^2.5.1",
    "istanbul": "^0.4.4",
    "tape": "^4.5.1",
    "tape-istanbul": "^1.0.3",
    "tape-run": "^2.1.4",
    "watchify": "^3.7.0",
    "zuul": "toddself/zuul"
  }
}
