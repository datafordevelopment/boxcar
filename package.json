{
  "name": "boxcar",
  "version": "0.0.1",
  "description": "A choo view for handling lists of data",
  "main": "boxcar.js",
  "scripts": {
    "start": "budo demo.js -o --title \"Boxcar Demo\" -P -l -- -t -g=es2020 -t sheetify/transform",
    "test": "zuul -- test/*.spec.js",
    "test:local": "zuul --local 8080 -- test/*.spec.js",
    "test:phantom": "zuul --phantom -- test/*.spec.js",
    "build": "browserify . > bundle.js"
  },
  "browserify": {
    "transforms": [
      "sheetify/transform",
      [
        "es2020",
        {
          "global": true
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com:toddself/boxcar.git"
  },
  "keywords": [
    "slickgrid",
    "table",
    "sortable",
    "choo",
    "grid"
  ],
  "author": "Todd Kennedy <todd@selfassembled.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/toddself/boxcar/issues"
  },
  "homepage": "https://github.com/toddself/boxcar#readme",
  "dependencies": {
    "@toddself/throw-down": "^0.1.7",
    "browserify-istanbul": "^2.0.0",
    "bubleify": "~0.5.0",
    "choo": "^2.2.0",
    "es2020": "^1.1.6",
    "insert-css": "^0.2.0",
    "min-document": "~2.18.0",
    "pull-notify": "^0.1.1",
    "pull-stream": "^3.4.3",
    "sheetify": "^5.0.1",
    "shortid": "^2.2.6",
    "string.fromcodepoint": "^0.2.1",
    "yo-yoify": "~3.0.0"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "browserify-istanbul": "^2.0.0",
    "budo": "^8.3.0",
    "istanbul": "^0.4.3",
    "keysim": "^1.3.0",
    "phantomjs": "^2.1.7",
    "tape": "^4.5.1",
    "watchify": "^3.7.0",
    "zuul": "toddself/zuul"
  }
}
